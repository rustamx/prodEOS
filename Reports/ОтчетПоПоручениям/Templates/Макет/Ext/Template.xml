		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	Поручение.Ссылка,
	Поручение.Дата,
	Поручение.Завершен,
	Поручение.Стартован,
	Поручение.Наименование,
	Поручение.Важность,
	Поручение.СрокИсполненияПроцесса КАК СрокИсполнения,
	Поручение.Автор,
	Поручение.ДатаНачала,
	Поручение.ДатаЗавершения,
	Поручение.Исполнитель,
	Поручение.ПометкаУдаления,
	Поручение.Проект
ИЗ
	БизнесПроцесс.Поручение КАК Поручение
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ПроцессыДляЗапуска КАК ПроцессыДляЗапуска
		ПО Поручение.Ссылка = ПроцессыДляЗапуска.БизнесПроцесс
ГДЕ
	(ТИПЗНАЧЕНИЯ(Поручение.Ссылка) &lt;&gt; ТИП(БизнесПроцесс.Поручение)
			ИЛИ Поручение.Ссылка.ЗадачаИсточник = ЗНАЧЕНИЕ(Задача.ЗадачаИсполнителя.ПустаяСсылка))
	И НЕ(ЕСТЬNULL(ПроцессыДляЗапуска.Состояние, ЗНАЧЕНИЕ(Перечисление.СостоянияПроцессовДляЗапуска.ПустаяСсылка)) = ЗНАЧЕНИЕ(Перечисление.СостоянияПроцессовДляЗапуска.ГотовКСтарту)
				И ЕСТЬNULL(ПроцессыДляЗапуска.ДатаОтложенногоСтарта, ДАТАВРЕМЯ(1, 1, 1)) &lt;&gt; ДАТАВРЕМЯ(1, 1, 1))
{ГДЕ
	(Поручение.Состояние = &amp;Состояние),
	(Поручение.Завершен = &amp;Завершен),
	(Поручение.ДатаНачала &lt;= &amp;ПериодОкончание
			И (Поручение.ДатаЗавершения &gt;= &amp;ПериодНачало
				ИЛИ Поручение.ДатаЗавершения = ДАТАВРЕМЯ(1, 1, 1))),
	(Поручение.ПометкаУдаления = ЛОЖЬ
			ИЛИ Поручение.ПометкаУдаления = &amp;ВыводитьПомеченныеНаУдаление),
	(ВЫБОР
			КОГДА ТИПЗНАЧЕНИЯ(&amp;Исполнитель) = ТИП(Справочник.Пользователи)
					И ИСТИНА В
						(ВЫБРАТЬ ПЕРВЫЕ 1
							ИСТИНА
						ИЗ
							РегистрСведений.ИсполнителиРолейИДелегаты КАК ИсполнителиРолейИДелегаты
						ГДЕ
							ИсполнителиРолейИДелегаты.РольПользователь = Поручение.Исполнитель
							И ИсполнителиРолейИДелегаты.ИсполнительДелегат = &amp;Исполнитель
							И ИсполнителиРолейИДелегаты.ИмяОбластиДелегирования В ("", "ПроцессыИЗадачи"))
				ТОГДА ИСТИНА
			КОГДА ТИПЗНАЧЕНИЯ(&amp;Исполнитель) = ТИП(Справочник.РабочиеГруппы)
					И ТИПЗНАЧЕНИЯ(Поручение.Исполнитель) = ТИП(Справочник.Пользователи)
					И Поручение.Исполнитель В
						(ВЫБРАТЬ
							СоставГрупп.Пользователь
						ИЗ
							РегистрСведений.СоставыГруппПользователей КАК СоставГрупп
						ГДЕ
							СоставГрупп.ГруппаПользователей = &amp;Исполнитель)
				ТОГДА ИСТИНА
			КОГДА ТИПЗНАЧЕНИЯ(&amp;Исполнитель) = ТИП(Справочник.РабочиеГруппы)
					И ТИПЗНАЧЕНИЯ(Поручение.Исполнитель) = ТИП(Справочник.ПолныеРоли)
					И Поручение.Исполнитель В
						(ВЫБРАТЬ
							ИсполнителиЗадач.РольИсполнителя
						ИЗ
							РегистрСведений.ИсполнителиЗадач КАК ИсполнителиЗадач
								ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрСведений.СоставыГруппПользователей КАК СоставГрупп
								ПО
									ИсполнителиЗадач.Исполнитель = СоставГрупп.Пользователь
						ГДЕ
							СоставГрупп.ГруппаПользователей = &amp;Исполнитель)
				ТОГДА ИСТИНА
			КОГДА ТИПЗНАЧЕНИЯ(&amp;Исполнитель) = ТИП(Справочник.ПолныеРоли)
					И ТИПЗНАЧЕНИЯ(Поручение.Исполнитель) = ТИП(Справочник.ПолныеРоли)
					И Поручение.Исполнитель = &amp;Исполнитель
				ТОГДА ИСТИНА
			ИНАЧЕ ЛОЖЬ
		КОНЕЦ),
	(Поручение.Автор = &amp;Автор)}</query>
