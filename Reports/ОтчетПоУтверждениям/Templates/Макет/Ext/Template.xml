		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	Утверждение.Ссылка,
	Утверждение.Дата,
	Утверждение.Завершен,
	Утверждение.Стартован,
	Утверждение.Наименование,
	Утверждение.Важность,
	Утверждение.СрокИсполненияПроцесса КАК СрокИсполнения,
	Утверждение.Автор,
	Утверждение.ДатаНачала,
	Утверждение.ДатаЗавершения,
	Утверждение.Исполнитель,
	Утверждение.ПометкаУдаления,
	Утверждение.РезультатУтверждения,
	Утверждение.НомерИтерации,
	Утверждение.Проект
ИЗ
	БизнесПроцесс.Утверждение КАК Утверждение
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ПроцессыДляЗапуска КАК ПроцессыДляЗапуска
		ПО Утверждение.Ссылка = ПроцессыДляЗапуска.БизнесПроцесс
ГДЕ
	НЕ(ЕСТЬNULL(ПроцессыДляЗапуска.Состояние, ЗНАЧЕНИЕ(Перечисление.СостоянияПроцессовДляЗапуска.ПустаяСсылка)) = ЗНАЧЕНИЕ(Перечисление.СостоянияПроцессовДляЗапуска.ГотовКСтарту)
				И ЕСТЬNULL(ПроцессыДляЗапуска.ДатаОтложенногоСтарта, ДАТАВРЕМЯ(1, 1, 1)) &lt;&gt; ДАТАВРЕМЯ(1, 1, 1))
{ГДЕ
	(Утверждение.Состояние = &amp;Состояние),
	(Утверждение.Завершен = &amp;Завершен),
	(Утверждение.ДатаНачала &lt;= &amp;ПериодОкончание
			И (Утверждение.ДатаЗавершения &gt;= &amp;ПериодНачало
				ИЛИ Утверждение.ДатаЗавершения = ДАТАВРЕМЯ(1, 1, 1))),
	(Утверждение.ПометкаУдаления = ЛОЖЬ
			ИЛИ Утверждение.ПометкаУдаления = &amp;ВыводитьПомеченныеНаУдаление)}</query>
	</dataSet>
