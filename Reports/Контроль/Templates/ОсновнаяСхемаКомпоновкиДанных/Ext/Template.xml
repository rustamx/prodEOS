		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	Контроль.Ссылка,
	Контроль.ПометкаУдаления,
	Контроль.Код,
	Контроль.Наименование,
	Контроль.ИсполнителиСтрокой,
	Контроль.Контролер,
	ВЫБОР
		КОГДА ТИПЗНАЧЕНИЯ(Контроль.Предмет) = ТИП(Справочник.ВеткиПереписки)
			ТОГДА Контроль.Предмет.КорневоеПисьмо
		ИНАЧЕ Контроль.Предмет
	КОНЕЦ КАК ПредметКонтроля,
	Контроль.СнятСКонтроля,
	Контроль.ПоставленНаКонтроль,
	Контроль.ДатаПостановкиНаКонтроль,
	Контроль.ДатаСнятияСКонтроля,
	Контроль.СрокИсполнения,
	ВЫБОР
		КОГДА Контроль.СрокИсполнения = ДАТАВРЕМЯ(1, 1, 1)
			ТОГДА 0
		КОГДА Контроль.СнятСКонтроля
			ТОГДА РАЗНОСТЬДАТ(Контроль.ДатаСнятияСКонтроля, Контроль.СрокИсполнения, ДЕНЬ)
		ИНАЧЕ РАЗНОСТЬДАТ(&amp;ТекущаяДата, Контроль.СрокИсполнения, ДЕНЬ)
	КОНЕЦ КАК ОсталосьДней,
	ВЫБОР
		КОГДА НАЧАЛОПЕРИОДА(Контроль.СрокИсполнения, ДЕНЬ) = НАЧАЛОПЕРИОДА(&amp;ТекущаяДата, ДЕНЬ)
			ТОГДА &amp;Сегодня
		КОГДА НАЧАЛОПЕРИОДА(Контроль.СрокИсполнения, ДЕНЬ) = НАЧАЛОПЕРИОДА(ДОБАВИТЬКДАТЕ(&amp;ТекущаяДата, ДЕНЬ, 1), ДЕНЬ)
			ТОГДА &amp;Завтра
	КОНЕЦ КАК СрокИстекаетДень,
	ВЫБОР
		КОГДА НАЧАЛОПЕРИОДА(Контроль.СрокИсполнения, НЕДЕЛЯ) = НАЧАЛОПЕРИОДА(&amp;ТекущаяДата, НЕДЕЛЯ)
			ТОГДА &amp;НаЭтойНеделе
		КОГДА НАЧАЛОПЕРИОДА(Контроль.СрокИсполнения, НЕДЕЛЯ) = НАЧАЛОПЕРИОДА(ДОБАВИТЬКДАТЕ(&amp;ТекущаяДата, НЕДЕЛЯ, 1), НЕДЕЛЯ)
			ТОГДА &amp;НаСледующейНеделе
	КОНЕЦ КАК СрокИстекаетНеделя
ИЗ
	Справочник.Контроль КАК Контроль
ГДЕ
	НЕ Контроль.ПометкаУдаления</query>
