		<expression>Значение</expression>
		<group>Период</group>
		<group>ПоказательПроцесса</group>
	</totalField>
	<totalField>
		<dataPath>ЗначениеНормализованное</dataPath>
		<expression>Максимум(ЗначениеНормализованное)</expression>
		<group>Период</group>
		<group>ПоказательПроцесса</group>
	</totalField>
