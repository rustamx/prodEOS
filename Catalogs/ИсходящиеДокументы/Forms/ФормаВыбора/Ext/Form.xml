			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>false</DynamicDataRead>
				<QueryText>ВЫБРАТЬ
	ИсходящиеДокументы.Документ КАК Ссылка,
	ИсходящиеДокументы.ПометкаУдаления,
	ИсходящиеДокументы.Код КАК ВнутреннийНомер,
	ИсходящиеДокументы.Наименование,
	ИсходящиеДокументы.ВидДокумента,
	ИсходящиеДокументы.ВопросДеятельности,
	ИсходящиеДокументы.ГрифДоступа,
	ИсходящиеДокументы.ДатаРегистрации,
	ИсходящиеДокументы.ДатаСоздания,
	ВЫРАЗИТЬ(ИсходящиеДокументы.КонтрагентыДляСписков КАК СТРОКА(1000)) КАК ПолучателиДляСписков,
	ИсходящиеДокументы.Дело,
	ИсходящиеДокументы.Заголовок,
	ИсходящиеДокументы.Зарегистрировал,
	ИсходящиеДокументы.НоменклатураДел,
	ИсходящиеДокументы.Организация,
	ИсходящиеДокументы.Ответственный,
	ИсходящиеДокументы.Подготовил,
	ИсходящиеДокументы.Подписал,
	ИсходящиеДокументы.Подразделение,
	ИсходящиеДокументы.РегистрационныйНомер,
	ВЫРАЗИТЬ(ИсходящиеДокументы.Содержание КАК СТРОКА(1000)) КАК Содержание,
	ИсходящиеДокументы.СрокИсполнения,
	ИсходящиеДокументы.Сумма,
	ИсходящиеДокументы.Валюта,
	ИсходящиеДокументы.РегистрационныйНомерИДата,
	ИсходящиеДокументы.ДатаСортировки,
	ИсходящиеДокументы.ЕстьФайлы
ИЗ
	РегистрСведений.ДанныеИсходящихДокументов КАК ИсходящиеДокументы
{ГДЕ
	(ИсходящиеДокументы.ВидДокумента = &amp;ВидДокумента),
	(ИСТИНА В
			(ВЫБРАТЬ
				ИСТИНА
			ИЗ
				Справочник.ИсходящиеДокументы.Получатели КАК ИсходящиеДокументыПолучатели
			ГДЕ
				ИсходящиеДокументыПолучатели.Ссылка = ИсходящиеДокументы.Документ
				И ИсходящиеДокументыПолучатели.Получатель = &amp;Получатель
				И ИсходящиеДокументыПолучатели.Отправлен = ИСТИНА))}</QueryText>
